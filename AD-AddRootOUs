New-ADOrganizationalUnit -Name "Monkey" -Path "DC=ATTACK,DC=LOCAL"-ProtectedFromAccidentalDeletion $Protect -verbose
New-ADOrganizationalUnit -Name "Cows" -Path "DC=ATTACK,DC=LOCAL"-ProtectedFromAccidentalDeletion $Protect -verbose
